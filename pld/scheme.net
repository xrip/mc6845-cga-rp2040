* MC6845-Based Color Text Video Adapter
* KiCad Legacy Netlist

*---------------- Components ----------------
* Component: MC6845 DIP40
* Pins: 1-40
COMPONENT MC6845 DIP40
  PIN 1 OUT MA0
  PIN 2 OUT MA1
  PIN 3 OUT MA2
  PIN 4 OUT MA3
  PIN 5 OUT MA4
  PIN 6 OUT MA5
  PIN 7 OUT MA6
  PIN 8 OUT MA7
  PIN 9 OUT MA8
  PIN 10 OUT MA9
  PIN 11 OUT MA10
  PIN 12 OUT MA11
  PIN 13 OUT MA12
  PIN 14 OUT MA13
  PIN 15 OUT RA0
  PIN 16 OUT RA1
  PIN 17 OUT RA2
  PIN 18 OUT RA3
  PIN 19 OUT RA4
  PIN 22 IN /RESET
  PIN 24 OUT DE
  PIN 25 OUT CURSOR
  PIN 26 OUT HSYNC
  PIN 27 OUT VSYNC
  PIN 28 IN CLK
ENDCOMPONENT

* Component: TextROM DIP28
COMPONENT TextROM DIP28
  PIN 1 IN A0
  PIN 2 IN A1
  PIN 3 IN A2
  PIN 4 IN A3
  PIN 5 IN A4
  PIN 6 IN A5
  PIN 7 IN A6
  PIN 8 IN A7
  PIN 9 IN A8
  PIN 10 IN A9
  PIN 11 IN A10
  PIN 12 IN A11
  PIN 13 IN A12
  PIN 14 IN A13
  PIN 15 OUT D0
  PIN 16 OUT D1
  PIN 17 OUT D2
  PIN 18 OUT D3
  PIN 19 OUT D4
  PIN 20 OUT D5
  PIN 21 OUT D6
  PIN 22 OUT D7
  PIN 27 PWR VCC
  PIN 28 PWR GND
ENDCOMPONENT

* Component: AttributeROM DIP28
COMPONENT AttributeROM DIP28
  PIN 1 IN A0
  PIN 2 IN A1
  PIN 3 IN A2
  PIN 4 IN A3
  PIN 5 IN A4
  PIN 6 IN A5
  PIN 7 IN A6
  PIN 8 IN A7
  PIN 9 IN A8
  PIN 10 IN A9
  PIN 11 IN A10
  PIN 12 IN A11
  PIN 13 IN A12
  PIN 14 IN A13
  PIN 15 OUT D0
  PIN 16 OUT D1
  PIN 17 OUT D2
  PIN 18 OUT D3
  PIN 19 OUT D4
  PIN 20 OUT D5
  PIN 21 OUT D6
  PIN 22 OUT D7
  PIN 27 PWR VCC
  PIN 28 PWR GND
ENDCOMPONENT

* Component: CharacterROM DIP28
COMPONENT CharacterROM DIP28
  PIN 1 IN A0
  PIN 2 IN A1
  PIN 3 IN A2
  PIN 4 IN A3
  PIN 5 IN A4
  PIN 6 IN A5
  PIN 7 IN A6
  PIN 8 IN A7
  PIN 15 IN A8
  PIN 16 IN A9
  PIN 17 IN A10
  PIN 18 IN A11
  PIN 19 IN A12
  PIN 15-22 OUT D0-D7
  PIN 27 PWR VCC
  PIN 28 PWR GND
ENDCOMPONENT

* Component: HC165 DIP16
COMPONENT HC165 DIP16
  PIN 1 IN D0
  PIN 2 IN D1
  PIN 3 IN D2
  PIN 4 IN D3
  PIN 5 IN D4
  PIN 6 IN D5
  PIN 7 IN D6
  PIN 8 PWR GND
  PIN 9 IN CLK
  PIN 10 IN /LOAD
  PIN 11 OUT QH
  PIN 16 PWR VCC
ENDCOMPONENT

* Component: CharacterPLD DIP20
COMPONENT CharacterPLD DIP20
  PIN 1 IN CLK
  PIN 2 IN PIXELS
  PIN 3 IN RESET
  PIN 4 IN HSYNCIN
  PIN 5 IN VSYNCIN
  PIN 6 IN CURSOR
  PIN 11 OUT CHARCLK
  PIN 14 OUT /LOAD
  PIN 15 OUT Q2
  PIN 18 OUT VIDEOOUT
  PIN 9 PWR GND
  PIN 20 PWR VCC
ENDCOMPONENT

* Component: AttributePLD DIP20
COMPONENT AttributePLD DIP20
  PIN 1 IN Clock
  PIN 2 IN FG0
  PIN 3 IN FG1
  PIN 4 IN FG2
  PIN 5 IN FG3
  PIN 6 IN BG0
  PIN 7 IN BG1
  PIN 8 IN BG2
  PIN 9 IN BG3
  PIN 10 PWR GND
  PIN 11 IN DE
  PIN 12 IN PIXEL
  PIN 17 OUT R
  PIN 18 OUT G
  PIN 19 OUT B
  PIN 20 OUT I
  PIN 20 PWR VCC
ENDCOMPONENT

